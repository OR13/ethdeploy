{
  "name": "ethdeploy",
  "version": "0.1.5",
  "description": "A highly configurable Ethereum smart-contract deployment staging utility.",
  "main": "index.js",
  "scripts": {
    "node:stop": "killall -9 -q node; exit 0",
    "example": "npm run node:stop && node ./example/index.js",
    "build:contracts": "cd tests && node ../node_modules/dapple/cmd/main.js build",
    "testrpc": "node ./node_modules/ethereumjs-testrpc/bin/testrpc",
    "test": "npm run node:stop && mocha tests/**.js -R spec --timeout 20000",
    "browserify": "node ./node_modules/browserify/bin/cmd.js --s ethdeploy ./index.js > ./dist/ethdeploy.js"
  },
  "bin": {
    "ethdeploy": "./bin/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/silentcicero/ethdeploy.git"
  },
  "keywords": [
    "ethdeploy",
    "ethereum",
    "eth",
    "solidity",
    "contract"
  ],
  "author": "Nick Dodson <thenickdodson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silentcicero/ethdeploy/issues"
  },
  "dependencies": {
    "array-equal": "^1.0.0",
    "bignumber-to-string": "^0.1.4",
    "chalk": "^1.1.3",
    "meow": "^3.7.0",
    "promise": "^7.1.1",
    "serialize-javascript": "^1.3.0",
    "web3": "^0.15.3"
  },
  "homepage": "https://github.com/silentcicero/ethdeploy#readme",
  "devDependencies": {
    "assert": "^1.4.1",
    "browserify": "^13.1.1",
    "ethdeploy-provider-http": "^0.1.0",
    "ethdeploy-provider-zero-client": "git+http://github.com/silentcicero/ethdeploy-provider-zero-client",
    "ethereumjs-testrpc": "^2.2.7",
    "ethereumjs-util": "^4.5.0",
    "mocha": "^2.5.3",
    "web3": "^0.15.3"
  }
}
